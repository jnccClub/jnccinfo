<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
 <class dynamic-update="true" lazy="false"
  name="org.jncc.base.application.Application" table="e_application">
  <id column="applicationID" name="applicationID">
   <generator class="assigned"/>
  </id>
<property column="createUser" 		generated="never"	 lazy="false"	 name="createUser" />
<property column="curentHandler" 	generated="never"	 lazy="false"	 name="curentHandler" />
<property column="courseName" 		generated="never"	 lazy="false"	 name="courseName" />
<property column="seats" 			generated="never"	 lazy="false"	 name="seats" />
<property column="begindate" 		generated="never"	 lazy="false"	 name="begindate" />
<property column="enddate" 			generated="never"	 lazy="false"	 name="enddate" />
<property column="begintime" 		generated="never"	 lazy="false"	 name="begintime" />
<property column="endtime" 			generated="never"	 lazy="false"	 name="endtime" />
<property column="weekday" 			generated="never"	 lazy="false"	 name="weekday" />
<property column="createdatetime" 	generated="never"	 lazy="false"	 name="createdatetime" />
<property column="floor" 			generated="never"	 lazy="false"	 name="floor" />
<property column="zone" 			generated="never"	 lazy="false"	 name="zone" />
<property column="commnet" 			generated="never"	 lazy="false"	 name="commnet" />
 </class>
</hibernate-mapping>
